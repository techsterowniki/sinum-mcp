# Sinum MCP Server Configuration
# Sinum API URL - default http://sinum.local/api/v1
SINUM_API_URL=http://sinum.local/api/v1

# API key for authorization in Sinum system
# WARNING: Replace 'your_api_key_here' with your actual API key
SINUM_API_KEY=your_api_key_here

# Sinum system hostname (optional)
SINUM_HOSTNAME=sinum.local

# MCP Server Configuration (optional)
MCP_SERVER_NAME=sinum-mcp
MCP_SERVER_VERSION=1.0.0
